{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["./src"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "useLongnameInNav": false,
        "default": {
            "outputSourceFiles": true
        }
    },
    "opts": {
        "destination": "./docs",
        "encoding": "utf8",
        "private": true,
        "recurse": true,
        "template": "node_modules/minami"
    }
}
